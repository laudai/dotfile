// 將按鍵繫結關係放在此檔案中以覆寫預設
[
  // Terminal
  {
    "key": "ctrl+a j",
    "command": "workbench.action.terminal.focusNext",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "ctrl+a k",
    "command": "workbench.action.terminal.focusPrevious",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "ctrl+a h",
    "command": "workbench.action.terminal.focusPreviousPane",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "ctrl+a l",
    "command": "workbench.action.terminal.focusNextPane",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "ctrl+a ctrl+k",
    "command": "workbench.action.terminal.kill",
    "when": "terminalFocus && terminalProcessSupported"
  },
  // Tab
  {
    "key": "ctrl+tab",
    "command": "workbench.action.nextEditor"
  },
  {
    "key": "ctrl+pageup",
    "command": "-workbench.action.previousEditor"
  },
  {
    "key": "ctrl+shift+tab",
    "command": "workbench.action.previousEditor"
  },
  {
    "key": "ctrl+pagedown",
    "command": "-workbench.action.nextEditor"
  },
  {
    "key": "ctrl+tab",
    "command": "-workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"
  },
  {
    "key": "ctrl+shift+tab",
    "command": "-workbench.action.quickOpenLeastRecentlyUsedEditorInGroup"
  },
  // Misc
  {
    "key": "Shift+Alt+m",
    "command": "editor.action.toggleMinimap"
  },
  {
    "key": "ctrl+shift+q",
    "command": "workbench.action.quit"
  },
  {
    "key": "ctrl+q",
    "command": "-workbench.action.quit"
  },
  {
    "key": "Shift+Alt+n",
    "command": "notifications.clearAll"
  },
  // 檢視: 快速開啟群組中上一個最近使用的編輯器
  // View: Quick Open Previous Recently Used Editor in Group
  {
    "key": "ctrl+k /",
    "command": "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"
  },
  // View: Quick Open Least Recently Used Editor in Group
  // 檢視: 快速開啟群組中最不常使用的編輯器
  {
    "key": "ctrl+k ctrl+/",
    "command": "workbench.action.quickOpenLeastRecentlyUsedEditorInGroup"
  },
  // Editor and Group
  {
    "key": "ctrl+k h",
    "command": "workbench.action.moveEditorToPreviousGroup"
  },
  {
    "key": "ctrl+alt+left",
    "command": "-workbench.action.moveEditorToPreviousGroup"
  },
  {
    "key": "ctrl+k l",
    "command": "workbench.action.moveEditorToNextGroup"
  },
  {
    "key": "ctrl+alt+right",
    "command": "-workbench.action.moveEditorToNextGroup"
  },
  {
    "key": "ctrl+k a",
    "command": "workbench.action.maximizeEditor"
  },
  // Focus on
  {
    "key": "shift+alt+e",
    "command": "workbench.files.action.focusFilesExplorer"
  },
  {
    "key": "ctrl+k space",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  {
    // 當焦點不在編輯區域時，將焦點移動到第一個 group 的第一個 editor
    "key": "ctrl+k v",
    "command": "workbench.action.firstEditorInGroup",
    "when": "!editorTextFocus"
  },
  /*
    ______     __                  _
   / ____/  __/ /____  ____  _____(_)___  ____  _____
  / __/ | |/_/ __/ _ \/ __ \/ ___/ / __ \/ __ \/ ___/
 / /____>  </ /_/  __/ / / (__  ) / /_/ / / / (__  )
/_____/_/|_|\__/\___/_/ /_/____/_/\____/_/ /_/____/
  Extensions
  */
  // bookmarks
  {
    "key": "ctrl+shift+j",
    "command": "bookmarks.jumpToNext",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+alt+l",
    "command": "-bookmarks.jumpToNext",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+j",
    "command": "-workbench.action.search.toggleQueryDetails",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+shift+k",
    "command": "bookmarks.jumpToPrevious",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+alt+j",
    "command": "-bookmarks.jumpToPrevious",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+k",
    "command": "-editor.action.deleteLines",
    "when": "textInputFocus && !editorReadonly"
  },
  // Settings Cycler keybindings
  {
    "key": "ctrl+shift+F3",
    "command": "settings.cycle.linenumber"
  },
  // blockman
  {
    // 開啟/關閉 blockman
    "key": "ctrl+alt+b",
    "command": "blockman.toggleEnableDisable"
  },
  /*
 _    ___              __ __           __    _           ___
| |  / (_)___ ___     / //_/__  __  __/ /_  (_)___  ____/ (_)___  ____ ______
| | / / / __ `__ \   / ,< / _ \/ / / / __ \/ / __ \/ __  / / __ \/ __ `/ ___/
| |/ / / / / / / /  / /| /  __/ /_/ / /_/ / / / / / /_/ / / / / / /_/ (__  )
|___/_/_/ /_/ /_/  /_/ |_\___/\__, /_.___/_/_/ /_/\__,_/_/_/ /_/\__, /____/
                             /____/                            /____/
  Vim Keybindings
  */
  {
    "key": "ctrl+alt+/",
    "command": "toggleVim"
  },
  {
    "key": "ctrl+k ctrl+k",
    "command": "extension.vim_ctrl+k",
    "when": "editorTextFocus && vim.active && vim.mode== 'Insert' && !inDebugRepl"
  },
  {
    "key": "alt+j",
    "command": "editor.action.moveLinesDownAction",
    "when": "editorTextFocus && !editorReadonly && vim.active"
  },
  {
    "key": "alt+k",
    "command": "editor.action.moveLinesUpAction",
    "when": "editorTextFocus && !editorReadonly && vim.active"
  },
  /*
    __  ___           ____  _____    __ __           __    _           ___
   /  |/  /___ ______/ __ \/ ___/   / //_/__  __  __/ /_  (_)___  ____/ (_)___  ____ ______
  / /|_/ / __ `/ ___/ / / /\__ \   / ,< / _ \/ / / / __ \/ / __ \/ __  / / __ \/ __ `/ ___/
 / /  / / /_/ / /__/ /_/ /___/ /  / /| /  __/ /_/ / /_/ / / / / / /_/ / / / / / /_/ (__  )
/_/  /_/\__,_/\___/\____//____/  /_/ |_\___/\__, /_.___/_/_/ /_/\__,_/_/_/ /_/\__, /____/
                                           /____/                            /____/
  MacOS Keybindings
  */
  // Focus at editor group
  {
    "key": "ctrl+shift+1",
    "command": "workbench.action.focusFirstEditorGroup"
  },
  {
    "key": "ctrl+shift+2",
    "command": "workbench.action.focusSecondEditorGroup"
  },
  {
    "key": "ctrl+shift+3",
    "command": "workbench.action.focusThirdEditorGroup"
  },
  {
    "key": "ctrl+shift+4",
    "command": "workbench.action.focusFourthEditorGroup"
  },
  {
    "key": "ctrl+shift+5",
    "command": "workbench.action.focusFifthEditorGroup"
  },
  {
    "key": "ctrl+shift+0",
    "command": "workbench.action.focusLastEditorGroup"
  },
  // Focus at editor index
  {
    "key": "cmd+1",
    "command": "workbench.action.openEditorAtIndex1"
    //"when": "isMac"
    // Add context keys for platforms #54894
    // https://github.com/microsoft/vscode/pull/54894
  },
  {
    "key": "cmd+1",
    "command": "-workbench.action.focusFirstEditorGroup"
  },
  {
    "key": "cmd+2",
    "command": "workbench.action.openEditorAtIndex2"
  },
  {
    "key": "cmd+2",
    "command": "-workbench.action.focusSecondEditorGroup"
  },
  {
    "key": "cmd+3",
    "command": "workbench.action.openEditorAtIndex3"
  },
  {
    "key": "cmd+3",
    "command": "-workbench.action.focusThirdEditorGroup"
  },
  {
    "key": "cmd+4",
    "command": "workbench.action.openEditorAtIndex4"
  },
  {
    "key": "cmd+4",
    "command": "-workbench.action.focusFourthEditorGroup"
  },
  {
    "key": "cmd+5",
    "command": "-workbench.action.focusFifthEditorGroup"
  },
  {
    "key": "cmd+5",
    "command": "workbench.action.openEditorAtIndex5"
  },
  {
    "key": "cmd+6",
    "command": "workbench.action.openEditorAtIndex6"
  },
  {
    "key": "cmd+6",
    "command": "-workbench.action.focusSixthEditorGroup"
  },
  {
    "key": "cmd+7",
    "command": "workbench.action.openEditorAtIndex7"
  },
  {
    "key": "cmd+7",
    "command": "-workbench.action.focusSeventhEditorGroup"
  },
  {
    "key": "cmd+8",
    "command": "workbench.action.openEditorAtIndex8"
  },
  {
    "key": "cmd+8",
    "command": "-workbench.action.focusEighthEditorGroup"
  },
  {
    "key": "cmd+9",
    "command": "-workbench.action.lastEditorInGroup"
  },
  {
    "key": "cmd+9",
    "command": "workbench.action.lastEditorInGroup"
  },
  {
    "key": "cmd+0",
    "command": "workbench.action.zoomReset"
  },
  {
    "key": "ctrl+0",
    "command": "workbench.action.focusSideBar",
    "when": "isMac"
  },
  // Misc
  {
    "key": "cmd+k cmd+o",
    "command": "workbench.action.files.openFileFolder",
    "when": "openFolderWorkspaceSupport"
  },
  {
    "key": "f2",
    "command": "renameFile",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "enter",
    "command": "explorer.openAndPassFocus",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsFolder && !inputFocus"
  },
  /*
    ____  _                          __   ____       ____            ____     _____      __  __  _
   / __ \(_)_____________ __________/ /  / __ \___  / __/___ ___  __/ / /_   / ___/___  / /_/ /_(_)___  ____ ______
  / / / / / ___/ ___/ __ `/ ___/ __  /  / / / / _ \/ /_/ __ `/ / / / / __/   \__ \/ _ \/ __/ __/ / __ \/ __ `/ ___/
 / /_/ / (__  ) /__/ /_/ / /  / /_/ /  / /_/ /  __/ __/ /_/ / /_/ / / /_    ___/ /  __/ /_/ /_/ / / / / /_/ (__  )
/_____/_/____/\___/\__,_/_/   \__,_/  /_____/\___/_/  \__,_/\__,_/_/\__/   /____/\___/\__/\__/_/_/ /_/\__, /____/
                                                                                                     /____/
  Discard Default Settings
  */
  {
    "key": "ctrl+1",
    "command": "-workbench.action.openEditorAtIndex1"
  },
  {
    "key": "ctrl+2",
    "command": "-workbench.action.openEditorAtIndex2"
  },
  {
    "key": "ctrl+3",
    "command": "-workbench.action.openEditorAtIndex3"
  },
  {
    "key": "ctrl+4",
    "command": "-workbench.action.openEditorAtIndex4"
  },
  {
    "key": "ctrl+5",
    "command": "-workbench.action.openEditorAtIndex5"
  },
  {
    "key": "ctrl+6",
    "command": "-workbench.action.openEditorAtIndex6"
  },
  {
    "key": "ctrl+7",
    "command": "-workbench.action.openEditorAtIndex7"
  },
  {
    "key": "ctrl+8",
    "command": "-workbench.action.openEditorAtIndex8"
  },
  {
    "key": "ctrl+9",
    "command": "-workbench.action.openEditorAtIndex9"
  },
  {
    "key": "ctrl+0",
    "command": "-workbench.action.lastEditorInGroup"
  },
  {
    "key": "enter",
    "command": "-renameFile",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "cmd+numpad0",
    "command": "-workbench.action.zoomReset"
  },
  {
    "key": "cmd+0",
    "command": "-workbench.action.focusSideBar"
  }
]