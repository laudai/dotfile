// 將按鍵繫結關係放在此檔案中以覆寫預設
[
  // Terminal
  {
    "key": "ctrl+a j",
    "command": "workbench.action.terminal.focusNext",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "ctrl+a k",
    "command": "workbench.action.terminal.focusPrevious",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "ctrl+a h",
    "command": "workbench.action.terminal.focusPreviousPane",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "ctrl+a l",
    "command": "workbench.action.terminal.focusNextPane",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "ctrl+a ctrl+k",
    "command": "workbench.action.terminal.kill",
    "when": "terminalFocus && terminalProcessSupported"
  },
  // Tab
  {
    "key": "ctrl+tab",
    "command": "workbench.action.nextEditor"
  },
  {
    "key": "ctrl+pageup",
    "command": "-workbench.action.previousEditor"
  },
  {
    "key": "ctrl+shift+tab",
    "command": "workbench.action.previousEditor"
  },
  {
    "key": "ctrl+pagedown",
    "command": "-workbench.action.nextEditor"
  },
  {
    "key": "ctrl+k /",
    "command": "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"
  },
  {
    "key": "ctrl+tab",
    "command": "-workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"
  },
  {
    "key": "ctrl+k ctrl+/",
    "command": "workbench.action.quickOpenLeastRecentlyUsedEditorInGroup"
  },
  {
    "key": "ctrl+shift+tab",
    "command": "-workbench.action.quickOpenLeastRecentlyUsedEditorInGroup"
  },
  // Misc
  {
    "key": "Shift+Alt+m",
    "command": "editor.action.toggleMinimap"
  },
  {
    "key": "ctrl+shift+q",
    "command": "workbench.action.quit"
  },
  {
    "key": "ctrl+q",
    "command": "-workbench.action.quit"
  },
  // Editor and Group
  {
    "key": "ctrl+k h",
    "command": "workbench.action.moveEditorToPreviousGroup"
  },
  {
    "key": "ctrl+alt+left",
    "command": "-workbench.action.moveEditorToPreviousGroup"
  },
  {
    "key": "ctrl+k l",
    "command": "workbench.action.moveEditorToNextGroup"
  },
  {
    "key": "ctrl+alt+right",
    "command": "-workbench.action.moveEditorToNextGroup"
  },
  {
    "key": "ctrl+a z",
    "command": "workbench.action.minimizeOtherEditors"
  },
  {
    "key": "ctrl+a n",
    "command": "workbench.action.evenEditorWidths"
  },
  // Focus on
  {
    "key": "shift+alt+e",
    "command": "workbench.files.action.focusFilesExplorer"
  },
  {
    "key": "ctrl+a space",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "ctrl+k v",
    "command": "workbench.action.firstEditorInGroup",
    "when": "!editorTextFocus"
  },
  // Plugin
  // Plugin-bookmarks
  {
    "key": "ctrl+alt+j",
    "command": "bookmarks.jumpToNext",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+alt+l",
    "command": "-bookmarks.jumpToNext",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+alt+k",
    "command": "bookmarks.jumpToPrevious",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+alt+j",
    "command": "-bookmarks.jumpToPrevious",
    "when": "editorTextFocus"
  },
  // Settings Cycler keybindings
  {
    "key": "ctrl+shift+F3",
    "command": "settings.cycle.linenumber",
  },
  // Vim Keybindings
  {
    "key": "ctrl+alt+/",
    "command": "toggleVim"
  },
  {
    "key": "ctrl+k ctrl+k",
    "command": "extension.vim_ctrl+k",
    "when": "editorTextFocus && vim.active && vim.mode== 'Insert' && !inDebugRepl"
  },
  {
    "key": "alt+j",
    "command": "editor.action.moveLinesDownAction",
    "when": "editorTextFocus && !editorReadonly && vim.active"
  },
  {
    "key": "alt+k",
    "command": "editor.action.moveLinesUpAction",
    "when": "editorTextFocus && !editorReadonly && vim.active"
  },
]